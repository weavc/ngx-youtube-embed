{
  "name": "ngx-youtube-embed",
  "version": "0.0.1",
  "description": "Embed youtube videos",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisWeaver1/ngx-youtube-embed"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prepare": "npm run packagr",
    "start": "ng serve",
    "build": "ng build --prod",
    "publish": "npm publish ./dist",
    "packagr": "ng-packagr -p package.json"
  },
  "ngPackage": {
    "whitelistedNonPeerDependencies": [
      "@types/youtube"
    ],
    "lib": {
      "entryFile": "public_api.ts"
    }
  },
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "peerDependencies": {
    "@angular/common": "^7.2.0",
    "@angular/core": "^7.2.0"
  },
  "dependencies": {
    "@types/youtube": "0.0.35"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/jasmine": "^3.3.9",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~12.12.0",
    "codelyzer": "^5.1.2",
    "core-js": "^2.5.4",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "material-design-lite": "^1.3.0",
    "ng-packagr": "^5.7.0",
    "protractor": "^5.4.2",
    "rxjs": "~6.3.3",
    "ts-node": "^8.5.2",
    "tsickle": "^0.37.0",
    "tslib": "^1.9.3",
    "tslint": "^5.15.0",
    "typescript": "~3.2.2",
    "zone.js": "^0.10.2"
